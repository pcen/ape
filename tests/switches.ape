module tests

func main(argv []string) {
	switch 3 {
	case 1:
		println(1)
	case 2:
		println(2)
	case 3:
		println(3)
	default:
		println(-1)
	}

	switch 2 {
	case 2:
		println(2)
		fallthrough
	case 1:
		println(1)
	default:
		println(999)
	}

	a := 10
	switch {
	case a == 10:
		println(10)
	default:
		println(-1)
	}
}
